{
    "version": 2,
    "builds": [
      {
        "src": "myproject/wsgi.py", 
        "use": "@vercel/python",
        "config": { "maxDuration": 10 }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "myproject/wsgi.py"
      }
    ],
    "outputDirectory": "static"
  }